{
  "name": "react-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "settings": {
      "react": {
        "version": "latest"
      }
    },
    "rules": {
      "eol-last": "warn",
      "max-len": "warn",
      "curly": "warn",
      "no-empty-function": "warn",
      "no-else-return": "warn",
      "no-use-before-define": "warn",
      "array-bracket-spacing": [
        "warn",
        "never"
      ],
      "block-spacing": "warn",
      "camelcase": "warn",
      "indent": [
        "warn",
        2
      ],
      "jsx-quotes": [
        "warn",
        "prefer-double"
      ],
      "keyword-spacing": [
        "error",
        {
          "before": true,
          "after": true
        }
      ],
      "max-lines": [
        "warn",
        {
          "max": 70,
          "skipBlankLines": true,
          "skipComments": true
        }
      ],
      "max-lines-per-function": [
        "error",
        {
          "max": 40,
          "skipComments": true
        }
      ],
      "no-inline-comments": "warn",
      "no-multiple-empty-lines": "warn",
      "no-trailing-spaces": "warn",
      "no-whitespace-before-property": "warn",
      "object-curly-spacing": [
        "warn",
        "always"
      ],
      "react/prop-types": [
        "warn"
      ],
      "react/sort-comp": [
        "warn"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-plugin-react": "^7.24.0"
  }
}
